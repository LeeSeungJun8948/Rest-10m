<mapper namespace="com.rest.app.comm.service.impl.CommMapper">
	<select id="getComm" resultType="com.rest.app.comm.vo.CommVO">
		SELECT code_id, code_id_nm
		  FROM COMTCCMMNCODE
		 WHERE code_id LIKE 'R%'
	</select>
	<select id="selectComm" resultType="com.rest.app.comm.vo.CommVO">
